:@template"@$//----------------------------------    [Color]
$cn:black;
$cb: white;
$cg: #f2f6f7;
$cg1: #dbdbdb;
$cg2: #fafafa;
$cg3: #9e9e9e;
$ca1:#03577d;
$ca2:#0a6f94;
$ca3:#dde7ec;
$cv1:#26803e;
$cv2:#74a93a;
$cv3:#9dbb40;
$cmo:#803d8b;
$cna:#e3b022;
$cfu:#be2e7e;


$cp1:#119cc0;
$cp2:#32ccb7;
$cp3:#f9fcfe;

$cFace: #3b5998;
$cTwee: #9AE4E8;
$cYoutube: #B50505;
$cReportes: #E49A39;

//--------------------------------------    [Fonts]
$f1:'movistar_headlinebold';
$f2:'movistar_headlineregular';
$f3:'movistar_textregular';
$f4:'movistar_textbold';
$f5:'movistar_textitalic';
$f6:'movistar_marketregular';
/*
 * Base structure
 */

/* Move down content because we have a fixed navbar that is 50px tall */
body {
  padding-top: 50px;
}


/*
 * Global add-ons
 */

.sub-header {
  padding-bottom: 10px;
  border-bottom: 1px solid #eee;
}

/*
 * Top navigation
 * Hide default border to remove 1px line.
 */
.navbar-fixed-top {
  border: 0;
}

/*
 * Sidebar
 */

/* Hide for mobile, show later */
.sidebar {
  display: none;
}
@media (min-width: 768px) {
  .sidebar {
    position: fixed;
    top: 51px;
    bottom: 0;
    left: 0;
    z-index: 1000;
    display: block;
    padding: 20px;
    overflow-x: hidden;
    overflow-y: auto; /* Scrollable contents if viewport is shorter than content. */
    background-color: #f5f5f5;
    border-right: 1px solid #eee;
  }
}

/* Sidebar navigation */
.nav-sidebar {
  margin-right: -21px; /* 20px padding + 1px border */
  margin-bottom: 20px;
  margin-left: -20px;
}
.nav-sidebar > li > a {
  padding-right: 20px;
  padding-left: 20px;
}
.nav-sidebar > .active > a,
.nav-sidebar > .active > a:hover,
.nav-sidebar > .active > a:focus {
  color: #fff;
  background-color: #428bca;
}


/*
 * Main content
 */

.main {
  padding: 20px;
}

.main .page-header {
  margin-top: 0;
}


/*
 * Placeholder dashboard ideas
 */

.placeholders {
  margin-bottom: 30px;
  text-align: center;
}
.placeholders h4 {
  margin-bottom: 0;
}
.placeholder {
  margin-bottom: 20px;
}
.placeholder img {
  display: inline-block;
  border-radius: 50%;
}
.navbar-inverse .navbar-collapse, .navbar-inverse .navbar-form{


}
.navbar-inverse{
    background:url(../images/sprite1.png) repeat-x;
}
.navbar-inverse .navbar-nav > li > a{
    color: $ca2;
    text-shadow:0 1px 1px $cb;
&:hover{
    color:$cn;
}
}
.navbar-brand{
      height: 57px;
      padding-left: 20px;
}
.timePlan{
        display: block;
        background:$cg1;
        height: 2px;
        width: 100%;
}
.logo{
        padding-top: 5px;
}
// Mi Estilos
.reporte{
            background:url(../images/bg_reporte.png) no-repeat;
            color:$cb;
            cursor: pointer;
            display: block;
            font-size: 1em;
            font-weight: normal;
            margin: 0;
            padding:5px 10px 10px 20px;
            position: fixed;
            top:56px;
            width: 180px;
            z-index: 30000;
}
#cont-appMerk{

// ..................................................................................................................... [ Titulos ]
.titu-1{
            color:$ca2;
            font-size: 0.8em;
            display: block;

}
.titu-2{
            color:$ca2;
            font-size: 1em;
            font-weight: normal;
            margin: 0;
}
.titu-3{
            color:$ca1;
            font-size: 1em;
            font-weight: normal;
            margin:0 0 10px;
}
.titu-4{
            background:url(../images/sprite1.png) repeat-x 0 -82px;
            box-shadow:0 1px 2px $cn;
            color:$cb;
            font-size: 1em;
            font-weight: normal;
            text-shadow:0 1px 2px #0071a1;
            margin:0;
            padding:10px;
}
.titu-5{
            background: $cp2;
            color:$cb;
            font-size: 1.2em;
            font-weight: normal;
            margin:0;
            padding:10px;
}
// ..................................................................................................................... [ Parrafos ]
.par-1{
            font-size: 0.9em;
            text-align: justify;
            height: auto;
            min-height: 50px;
}
// ..................................................................................................................... [ Labels ]
.label-Facebook{
               background: $cFace;
              border: none;
              color: $cb;
              padding:10px;
              margin-top: -10px;
              position: relative;
              top: 18px;
}
.label-twee{
               background: $cTwee;
              border: none;
              color: $ca2;
              padding:10px;
              margin-top: -10px;
              position: relative;
              top: 18px;
}
.label-youtube{
              background: $cYoutube;
              border:none;
              color: $cb;
              padding:10px;
              margin-top: -10px;
              position: relative;
              top: 18px;
}
// ..................................................................................................................... [ Imagenes ]
.img1{
            padding: 10px 0;
img{
            width:100%;
}
}
// ..................................................................................................................... [ Formularios ]
.form-1{
            background: $cg2;
            padding: 20px;
            margin: 20px 0;
.input-1{
            border:none;
            margin: 10px;
            padding: 5px;
            width: 35%;
}
.input-2{
            display: inline-block;
            border:1px solid $ca3;
            color: $ca2;
            font-size: 0.9em;
            margin: 20px 0;
}
.textarea-1{
            border:none;
            height: 150px;
            margin: 10px;
            padding: 5px;
            width: 45%;
}
.titu-form{
            display: inline-block;
            color: $ca2;
            padding: 0 20px;
            width: 130px;
}
.titu-form-2{
            display: inline-block;
            color: $ca2;
            padding: 0 20px;
            width: auto;
}
.select-1{
              border:1px solid $ca3;
              color: $ca2;
              font-size: 0.9em;
              margin: 20px 0;
}
.btn-1{
            margin: 0 0 0 100px;
            width: 12%;
}
}
label.error {
            color: rgb(142, 5, 250);
            display: block;
            font-size: 0.9em;
            font-weight: normal;
            margin:-10px 0 0 18px;

}
// ..................................................................................................................... [ Estructura ]
#contReporte{
        display: none;
}
.cont-filtros{
        background: $cg2;
        padding: 10px 0;
        margin-top:30px;
.filtros{
        margin: 10px 40px 0;
}
}
.box-cont{
        padding:0;
        margin: 0 5px;
        width: 19%;
}
.box-post{
            border: 1px solid $cg1;
            box-shadow: 0 2px 3px $cg1;
            margin: 10px 0;
            padding: 10px;
            position: relative;
            overflow: hidden;
.aprobado{
            margin: 0 0 10px;
  }
.msnRepor{
            background: $cb;
            padding:10px;
            position: absolute;
            top: -179px;
            left: 0;
            z-index: 20;
.box-msn{
            height: 100px;
            width: 100%;
}
}
}
// ................................................................................................... [ Post Facebook]
.eFacebook{
.msnRepor{
              border-bottom: 2px solid $cFace;
}
}

// ................................................................................................... [ Post Twitter]
.eTwee{
.msnRepor{
              border-bottom: 2px solid $cTwee;
}
}

// ................................................................................................... [ Post Youtube]
.eYoutube{
.msnRepor{
              border-bottom: 2px solid $cYoutube;
}
}
}

//.................................................................................................................................................................................................................. 1046px
@media (max-width: 1046px) {
#cont-appMerk{

.box-cont{
             // width: 18.9%;
              width: 32%;
}
.box-cont:nth-last-child(1){
               width: 48.5%;
}
.box-cont:nth-last-child(2){
                 width: 48.5%;
}
}

}
//.................................................................................................................................................................................................................. 1046px
@media (max-width: 800px) {
#cont-appMerk{

.box-cont{
             // width: 18.9%;
               width: 48.5%;
}
.box-cont:nth-last-child(1){
               width: 98%;
}
}
}

//.................................................................................................................................................................................................................. 1046px
@media (max-width: 768px) {
#cont-appMerk{
.box-cont{
               width: 98%;
}
.box-cont:nth-last-child(2){
               width: 98%;
}
.box-cont:nth-last-child(1){
               width: 98%;
}
}
}

@media (min-width: 768px) {
  .main {
    padding-right: 40px;
    padding-left: 40px;
  }
